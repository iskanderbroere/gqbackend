    "compression": "^1.7.2",
    "graphql-yoga": "1.4.3",
    "jsonwebtoken": "8.2.0",
    "prisma-binding": "1.5.16"
  },
  "devDependencies": {
    "dotenv": "5.0.1",
    "graphql-cli": "2.15.8",
    "nodemon": "1.17.1",
    "npm-run-all": "4.1.2",
    "prisma": "1.3.5"
  }
}
